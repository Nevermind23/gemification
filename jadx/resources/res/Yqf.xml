<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:state_checked="false">
        <shape android:shape="rectangle">
            <size android:height="@dimen/toggle_height" android:width="@dimen/toggle_width"/>
            <corners android:radius="12dp"/>
            <solid android:color="@color/color_invert_component_tr_5"/>
        </shape>
    </item>
    <item android:state_enabled="false" android:state_checked="true">
        <shape android:shape="rectangle">
            <size android:height="@dimen/toggle_height" android:width="@dimen/toggle_width"/>
            <corners android:radius="12dp"/>
            <solid android:color="?attr/color_primary_solid_50"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <size android:height="@dimen/toggle_height" android:width="@dimen/toggle_width"/>
            <corners android:radius="12dp"/>
            <solid android:color="?attr/color_invert_component_tr_50"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <size android:height="@dimen/toggle_height" android:width="@dimen/toggle_width"/>
            <corners android:radius="12dp"/>
            <solid android:color="?attr/color_primary_solid_df_500"/>
        </shape>
    </item>
</selector>
