<?xml version="1.0" encoding="utf-8"?>
<shortcuts xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" xmlns:aapt="http://schemas.android.com/aapt">
    <shortcut android:icon="@drawable/ic_app_shortcuts_qr" android:enabled="true" android:shortcutId="qr_pay" android:shortcutShortLabel="@string/qr_pay" android:shortcutLongLabel="@string/qr_pay">
