<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="?attr/color_primary_solid_200" android:dashWidth="4dp" android:dashGap="4dp"/>
            <solid android:color="@color/color_invert_component_tr_5"/>
            <corners android:radius="@dimen/layer_radius_16"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1dp" android:color="?attr/color_primary_solid_200" android:dashWidth="4dp" android:dashGap="4dp"/>
            <solid android:color="?attr/color_invert_background_layer"/>
            <corners android:radius="@dimen/layer_radius_16"/>
        </shape>
    </item>
</selector>
