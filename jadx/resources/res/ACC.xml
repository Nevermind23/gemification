<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/color_negative_solid_200"/>
            <solid android:color="@color/color_negative_solid_50"/>
            <corners android:radius="@dimen/layer_radius_16"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/color_negative_solid_200"/>
            <solid android:color="@color/color_negative_solid_25"/>
            <corners android:radius="@dimen/layer_radius_16"/>
        </shape>
    </item>
</selector>
