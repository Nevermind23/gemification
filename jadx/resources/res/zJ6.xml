<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="?attr/color_black_tr_400" android:endColor="?attr/color_black_tr_50" android:angle="270" android:type="linear"/>
        </shape>
    </item>
    <item android:gravity="bottom" android:bottom="0dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="?attr/color_black_tr_400" android:endColor="?attr/color_black_tr_50" android:angle="90" android:type="linear"/>
        </shape>
    </item>
</layer-list>
